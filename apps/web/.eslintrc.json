{
  "root": true,
  "extends": "next/core-web-vitals",
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "@typescript-eslint/no-explicit-any": "warn",
    "prettier/prettier": "off"
  },
  "ignorePatterns": [
    "test/**/*",
    "*.test.ts",
    "*.test.tsx"
  ]
}